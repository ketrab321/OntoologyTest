<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#"
     xml:base="http://www.semanticweb.org/ontologies/2021/2/pizza.owl"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl">
        <rdfs:comment>A pizza ontology that describes various pizzas based on their toppings</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasBase -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasBase">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#isBaseOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#Pizza_BS"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#PizzaBase_BS"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasCountrOriginOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasCountrOriginOf"/>
    


    <!-- http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasIngredient -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasIngredient">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#isIngredientOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasSpiciness -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasSpiciness">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#SpicinessValuePartition_BS"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasTopping -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasTopping">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#isToppingOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#Pizza_BS"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#PizzaTopping_BS"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2021/2/pizza.owl#isBaseOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#isBaseOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#isIngredientOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#PizzaBase_BS"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#Pizza_BS"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2021/2/pizza.owl#isIngredientOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#isIngredientOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2021/2/pizza.owl#isToppingOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#isToppingOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#isIngredientOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#PizzaTopping_BS"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#Pizza_BS"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasCalorificContentValue -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasCalorificContentValue">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/ontologies/2021/2/pizza.owl#AmericanaHotPizza_BS -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#AmericanaHotPizza_BS">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#NamedPizza_BS"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#JalapenoPepperTopping_BS"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#MozzarellaTopping_BS"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#PepperoniTopping_BS"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#TomatoTopping_BS"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#JalapenoPepperTopping_BS"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#MozzarellaTopping_BS"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#PepperoniTopping_BS"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#TomatoTopping_BS"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A pizza that only has Mozarella and Tomato toppings</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2021/2/pizza.owl#AmericanaPizzs_BS -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#AmericanaPizzs_BS">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#NamedPizza_BS"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#MozzarellaTopping_BS"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#PepperoniTopping_BS"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#TomatoTopping_BS"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#MozzarellaTopping_BS"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#PepperoniTopping_BS"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#TomatoTopping_BS"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A pizza that only has Mozarella and Tomato toppings</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2021/2/pizza.owl#AnchovyTopping_BS -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#AnchovyTopping_BS">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#SeafoodTopping_BS"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasSpiciness"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#Mild_BS"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2021/2/pizza.owl#CaperTopping_BS -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#CaperTopping_BS">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#VegetablesTopping_BS"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasSpiciness"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#Medium_BS"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2021/2/pizza.owl#CheeseTopping_BS -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#CheeseTopping_BS">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#PizzaTopping_BS"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#MeatTopping_BS"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#SeafoodTopping_BS"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#VegetablesTopping_BS"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2021/2/pizza.owl#CheeseyPizza_BS -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#CheeseyPizza_BS">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#Pizza_BS"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasTopping"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#CheeseTopping_BS"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2021/2/pizza.owl#Country_BS -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#Country_BS">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#America_BS"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#England_BS"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#France_BS"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#Germany_BS"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#Italy_BS"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2021/2/pizza.owl#DeepPanBase_BS -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#DeepPanBase_BS">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#PizzaBase_BS"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#ThinAndCrispyBase_BS"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2021/2/pizza.owl#FourCheesePizza_BS -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#FourCheesePizza_BS">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#Pizza_BS"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasTopping"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">4</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#CheeseTopping_BS"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2021/2/pizza.owl#GreenPepperTopping_BS -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#GreenPepperTopping_BS">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#PepperTopping_BS"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasSpiciness"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#Medium_BS"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2021/2/pizza.owl#HamTopping_BS -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#HamTopping_BS">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#MeatTopping_BS"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasSpiciness"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#Mild_BS"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2021/2/pizza.owl#HighCaloriePizza_BS -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#HighCaloriePizza_BS">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#Pizza_BS"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasCalorificContentValue"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">400</xsd:minInclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2021/2/pizza.owl#Hot_BS -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#Hot_BS">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#SpicinessValuePartition_BS"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2021/2/pizza.owl#InterestingPizza_BS -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#InterestingPizza_BS">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#Pizza_BS"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasTopping"/>
                        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">3</owl:minCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2021/2/pizza.owl#JalapenoPepperTopping_BS -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#JalapenoPepperTopping_BS">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#PepperTopping_BS"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasSpiciness"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#Hot_BS"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2021/2/pizza.owl#LowCaloriePizza_BS -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#LowCaloriePizza_BS">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#Pizza_BS"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasCalorificContentValue"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:maxExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">400</xsd:maxExclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2021/2/pizza.owl#MargheritaPizza_BS -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#MargheritaPizza_BS">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#NamedPizza_BS"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#MozzarellaTopping_BS"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#TomatoTopping_BS"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2021/2/pizza.owl#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
